// Place your key bindings in this file to override the defaultsauto[]
[
    // Split editor horizontally
    {
        "key": "ctrl+[Backquote]",
        "command": "workbench.action.splitEditorOrthogonal"
    },
    // Split editor vertically
    {
        "key": "ctrl+shift+[Backquote]",
        "command": "workbench.action.splitEditor"
    },
    // Create a new file in file explorer at selected location
    {
        "key": "ctrl+n",
        "command": "explorer.newFile", "when": "explorerViewletFocus"
    },
    // Navigate back in history
    {
        "key": "alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+alt+-",
        "command": "-workbench.action.navigateBack"
    },
    // Navigate forward in history
    {
        "key": "alt+right",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    },
    // Create a new internal terminal "CTRL + SHIFT + ´"
    {
        "key": "ctrl+shift+[Equal]",
        "command": "workbench.action.terminal.new"
    },
    // Activate/Focus internal terminal "CTRL + ´"
    {
        "key": "ctrl+[Equal]",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    // Focus editor group above
    {
        "key": "ctrl+m ctrl+i",
        "command": "workbench.action.focusAboveGroup"
    },
    {
        "key": "ctrl+k ctrl+up",
        "command": "-workbench.action.focusAboveGroup"
    },
    // Focus editor group below
    {
        "key": "ctrl+m ctrl+k",
        "command": "workbench.action.focusBelowGroup"
    },
    {
        "key": "ctrl+k ctrl+down",
        "command": "-workbench.action.focusBelowGroup"
    },
    // Focus editor group left
    {
        "key": "ctrl+m ctrl+l",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+k ctrl+left",
        "command": "-workbench.action.focusLeftGroup"
    },
    // Focus editor group right
    {
        "key": "ctrl+m ctrl+j",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+k ctrl+right",
        "command": "-workbench.action.focusRightGroup"
    },
    // Select a Debug-Configuration and start it
    {
        "key": "alt+j",
        "command": "workbench.action.debug.selectandstart"
    }
]